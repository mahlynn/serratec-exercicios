package br.com.residencia.poo.exercicios;

public class ListaExercicioCinco {
	
	public static void main(String[] args) {
		
		int resultadoA, resultadoB;
		
		resultadoA = 2 + 3 - ( 5 * 8 ) - 4 + 354 - 521 + ( 7 * 66 );
		
		System.out.println("Expressão A: 2 + 3 - 5 * 8 - 4 + 354 - 521 + 7 * 66");
		System.out.println("> Resultado: "+ resultadoA);
		
		System.out.printIn("================================================");
		
		resultadoB = 2 + 7 * ( 14 - 21 ) + 28 * 3 * 42 + 740 - ( 156 + 4 + 40 ) * 9;
		
		System.out.println("Expressão B: 2 + 7 * ( 14 - 21 ) + 28 * 3 * 42 + 740 - ( 156 + 4 + 40 ) * 9");
		System.out.println("> Resultado: "+ resultadoB);
		
		
	}

}
